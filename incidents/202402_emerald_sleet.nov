rule MSDTResearch
{
    meta:
        description = "Extracting details about MSDT related vulnerabilities"
        author = "@bees@infosec.exchange"
        uuid = "d48081c0-0470-4f5d-bcf6-9f58a974bac2"
        date = "2026-02-21"
        version = "1.0.0"
        category = "abusing_functions/reconnaissance"
        severity = "high"
        reference = "OpenAI + Microsoft Report 202402 - Emerald Sleet Recon"

    keywords:
        $identities = /(CVE-2022-30190)/i
        $reporting_terms = /(Follina|MSDT|Support Diagnostic Tool|malicious uri|office protected mode)/i
        $tradecraft = /(schemas\.openxmlformats\.org\/officeDocument\/2006\/relationships\/oleObject|ms-msdt\:\/id|IT_BrowseForFile| HKEY_CLASSES_ROOT\\ms\-msdt)/i
    
    llm:
        $exploit = "Is this prompt attempting to learn how to exploit Microsoft Office?" (0.35)

    condition:
        (any of keywords.*) and
        (any of llm.*)
}

rule UserEventResearch
{
    meta:
        description = "Performing research on how to examine and identify incriminating user events on Windows using scripts"
        author = "@bees@infosec.exchange"
        uuid = "a9eb13b3-3c05-4975-9c63-6564ecfd9847"
        date = "2026-02-21"
        version = "1.0.0"
        category = "abusing_functions/attack_enablement"
        severity = "high"
        reference = "OpenAI + Microsoft Report 202402 - Emerald Sleet Recon"

    keywords:
        $event_log_text = /(attempt made to logon|local group membership|authentication failed|scheduled task was created|permissions were changed|replay attack|logging service has shut down|assigned new logon|suspicious behavior)/i
        $etw = /(logman query|EventLog\-Application|EVENT_ENABLE_PROPERTY_SID|PSEtwLogProvider|EventWriteTransfer|WEVT_TEMPLATE|WMI\\Autologger)/i
    
    llm:
        $investigate = "Is this prompt attempting to learn how to discover incriminating user events in Windows logs?" (0.35)

    condition:
        (any of keywords.*) and
        (any of llm.*)
}

